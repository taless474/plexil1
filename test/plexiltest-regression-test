callCmd()
{
  echo
  echo  
  echo "$*"
  $*  
  sleep 2
  echo  
  echo  
}

callCmd "$PLEXIL_HOME/scripts/plexiltest -p $PLEXIL_HOME/examples/basic/SimpleAssignment.plx" 		#current script should engage in examples
callCmd "$PLEXIL_HOME/scripts/plexiltest -q -p $PLEXIL_HOME/examples/basic/SimpleAssignment.plx" 		#ignore unchecked warnings
callCmd "$PLEXIL_HOME/scripts/plexiltest -v -p $PLEXIL_HOME/examples/basic/SimpleAssignment.plx"		#allocated viewer and plan
callCmd "$PLEXIL_HOME/scripts/plexiltest -v"							#should not run either
callCmd "$PLEXIL_HOME/scripts/plexiltest  -v -n -p $PLEXIL_HOME/examples/basic/SimpleAssignment.plx" 	#require argument
callCmd "$PLEXIL_HOME/scripts/plexiltest -v -n 1 -p $PLEXIL_HOME/examples/basic/SimpleAssignment.plx" 	#warnings although still run... otherwise block user's control
callCmd "$PLEXIL_HOME/scripts/plexiltest -ch -p $PLEXIL_HOME/examples/basic/SimpleAssignment.plx" 	#check plan with static checker
callCmd "$PLEXIL_HOME/scripts/plexiltest" 								#require argument error
callCmd "$PLEXIL_HOME/scripts/plexiltest -p abc.plx"							#plan not found
callCmd "$PLEXIL_HOME/scripts/plexiltest -n 1000 -p /home/jho4/workspace/PLEXIL/examples/basic/SimpleAssignment.plx" #error
callCmd "$PLEXIL_HOME/scripts/plexiltest -p $PLEXIL_HOME/examples/basic/SimpleAssignment.plx -d thatbug" 	#debug file does not exist, exit
callCmd "$PLEXIL_HOME/scripts/plexiltest -p $PLEXIL_HOME/examples/basic/SimpleAssignment.plx -l boo -l hoo -l foo -l bar" #library files do not exist exit

